<!DOCTYPE html>
<html>
  <head>
    <title>Carvalho Auto Center</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="img/favicon.ico" rel="icon">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&icon_names=home" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500&display=swap" rel="stylesheet">
    <!-- Corrigido para Material Symbols -->
    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined&display=block" rel="stylesheet">
    <%= stylesheet_link_tag "bootstrap.min", media: "all", 'data-turbo-track': "reload" %>
    <%= stylesheet_link_tag "style", media: "all", 'data-turbo-track': "reload" %>
    <%= stylesheet_link_tag "application", media: "all", 'data-turbo-track': "reload" %>
    <%= javascript_importmap_tags %>
    <% # DependÃªncias JS %>
    <% # App JS do projeto %>
    <%= javascript_include_tag "https://code.jquery.com/jquery-3.4.1.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/wow/wow.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/easing/easing.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/waypoints/waypoints.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/counterup/counterup.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/owlcarousel/owl.carousel.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/tempusdominus/js/moment.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/tempusdominus/js/moment-timezone.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "/assets/javascripts/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "main", 'data-turbo-track': "reload" %>
    <%= javascript_include_tag "application", 'data-turbo-track': "reload" %>
  </head>
  <body>
    <%= yield %>
  </body>
</html>
